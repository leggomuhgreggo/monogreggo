{
  "version": 2,
  "projects": {
    "expoapp": {
      "root": "apps/expoapp",
      "sourceRoot": "apps/expoapp/src",
      "projectType": "application",
      "generators": {},
      "targets": {
        "bundle": {
          "executor": "nx-react-native-expo:bundle",
          "options": {}
        },
        "start": {
          "executor": "nx-react-native-expo:start",
          "options": {}
        },
        "run-ios": {
          "executor": "nx-react-native-expo:run-ios",
          "options": {}
        },
        "run-android": {
          "executor": "nx-react-native-expo:run-android",
          "options": {}
        },
        "run-web": {
          "executor": "nx-react-native-expo:run-web",
          "options": {}
        },
        "publish": {
          "executor": "nx-react-native-expo:publish",
          "options": {}
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/expoapp/**/*.{js,ts,tsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/expoapp"
          ],
          "options": {
            "jestConfig": "apps/expoapp/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nextapp": {
      "root": "apps/nextapp",
      "sourceRoot": "apps/nextapp",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/nextapp",
            "outputPath": "dist/apps/nextapp"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "nextapp:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "nextapp:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "nextapp:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/nextapp"
          ],
          "options": {
            "jestConfig": "apps/nextapp/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/nextapp/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
    "nextapp-e2e": {
      "root": "apps/nextapp-e2e",
      "sourceRoot": "apps/nextapp-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nextapp-e2e/cypress.json",
            "devServerTarget": "nextapp:serve",
            "tsConfig": "apps/nextapp-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nextapp:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/nextapp-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "vanilla-rn": {
      "root": "apps/vanilla-rn",
      "sourceRoot": "apps/vanilla-rn/src",
      "projectType": "application",
      "targets": {
        "start": {
          "executor": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx start vanilla-rn"
          }
        },
        "run-ios": {
          "executor": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "executor": "@nrwl/react-native:bundle",
          "outputs": [
            "apps/vanilla-rn/build"
          ],
          "options": {
            "entryFile": "apps/vanilla-rn/src/main.tsx",
            "platform": "ios",
            "bundleOutput": "dist/apps/vanilla-rn/ios/main.jsbundle"
          }
        },
        "run-android": {
          "executor": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "executor": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/vanilla-rn/android/app/build/outputs/bundle",
            "apps/vanilla-rn/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "executor": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/vanilla-rn/src/main.tsx",
            "platform": "android",
            "bundleOutput": "dist/apps/vanilla-rn/android/main.jsbundle"
          }
        },
        "sync-deps": {
          "executor": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "ensure-symlink": {
          "executor": "@nrwl/react-native:ensure-symlink",
          "options": {}
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/vanilla-rn/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/vanilla-rn"
          ],
          "options": {
            "jestConfig": "apps/vanilla-rn/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "vanilla-rn-e2e": {
      "root": "apps/vanilla-rn-e2e",
      "sourceRoot": "apps/vanilla-rn-e2e/src",
      "projectType": "application",
      "targets": {
        "build-ios": {
          "executor": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "ios.sim.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release"
            }
          }
        },
        "test-ios": {
          "executor": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "ios.sim.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release"
            }
          }
        },
        "build-android": {
          "executor": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "android.emu.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release"
            }
          }
        },
        "test-android": {
          "executor": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "android.emu.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/vanilla-rn-e2e/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react-native"
  },
  "defaultProject": "vanilla-rn",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  }
}
